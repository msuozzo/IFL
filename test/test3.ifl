ITEM Medicine:
	START: 
		ADD {INTEGER heal_value = 25} TO SELF
	ACTIONS: 
		"get" 
			REMOVE medicine FROM LOCATION ON medicine
			ADD medicine TO PLAYER
		"eat" : 
			IF PLAYER HAS Medicine 
				EXECUTE update ON health ON PLAYER WITH heal_value
			REMOVE medicine from PLAYER
		"drop" : 
			IF PLAYER HAS Medicine 
			REMOVE Medicine from PLAYER
