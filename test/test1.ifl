TRAIT Heatlth: 
  START: 
		ADD{INTEGER max = 200} TO SELF
	FUNCTION: 
		update1 WITH amount: 
			INCREASE current ON SELF BY amount
			IF current > max ON SELF: 
				SET current ON SELF TO max ON SELF

		update2 WITH amount: 
			DECREASE current ON SELF BY amount
			IF current < 0 
				SET current ON SELF TO 0 ON SELF 
